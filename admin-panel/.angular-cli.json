{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "admin-panel"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "login",
        "main.css",
        "bootstrap.min.css"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "js/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "js/fastclick.js",
        "js/nprogress.js",
        "js/Chart.min.js",
        "js/gauge.min.js",
        "js/bootstrap-progressbar.min.js",
        "js/icheck.min.js",
        "js/skycons.js",
        "js/jquery.flot.js",
        "js/jquery.flot.pie.js",
        "js/jquery.flot.time.js",
        "js/jquery.flot.stack.js",
        "js/jquery.flot.resize.js",
        "js/jquery.flot.orderBars.js",
        "js/jquery.flot.spline.min.js",
        "js/curvedLines.js",
        "js/date.js",
        "js/jquery.vmap.js",
        "js/jquery.vmap.world.js",
        "js/jquery.vmap.sampledata.js",
        "js/moment.min.js",
        "js/daterangepicker.js",
        "js/helpers/smartresize.js",
        "../node_modules/chart.js/src/chart.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
