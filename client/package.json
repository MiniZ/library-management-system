{
  "name": "evaluation",
  "description": "Full featured Vue.js starter project for browser apps",
  "version": "1.0.0",
  "author": "Matúš Čongrády <m.congrady@gmail.com>",
  "main": "./src/index.js",
  "keywords": [
    "Vue",
    "Vuex",
    "Vue-i18n",
    "GraphQL",
    "Vue starter"
  ],
  "scripts": {
    "build-dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --hide-modules",
    "analyze": "rimraf dist && cross-env NODE_ENV=analyze webpack --hide-modules",
    "serve:build": "npx http-server ./dist -p 3001 --cors --push-state",
    "clean": "rimraf node_modules package-lock.json dist yarn.lock",
    "lint": "npx eslint .",
    "check": "npx ncu",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "apollo-client": "^1.9.0",
    "babel-polyfill": "^6.23.0",
    "graphql-tag": "^2.4.2",
    "lodash": "^4.17.4",
    "vue": "^2.4.2",
    "vue-apollo": "^2.1.0-beta.20",
    "vue-i18n": "^7.1.1",
    "vue-property-decorator": "^5.2.1",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "chalk": "^2.0.1",
    "critical": "^0.9.0",
    "cross-env": "^5.0.4",
    "css-loader": "^0.28.4",
    "cssnano-cli": "^1.0.5",
    "eslint": "^4.4.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.5.3",
    "npm-check-updates": "^2.12.1",
    "on-build-webpack": "^0.1.0",
    "prettier": "^1.5.3",
    "progress-bar-webpack-plugin": "^1.10.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "spa-http-server": "^0.9.0",
    "style-loader": "^0.18.2",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.6.1"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.2.0"
  }
}
